{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist/client/browser",
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "env": {
        "API_URL": "https://perla-accessories-backend.vercel.app"
    }
}