{
    "version": 2,
    "framework": "angular",
    "buildCommand": "npm run build:ssr",
    "functions": {
        "dist/client/server/server.mjs": {
            "runtime": "nodejs20.x"
        }
    },
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/dist/client/browser/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/client/server/server.mjs"
        }
    ]
}