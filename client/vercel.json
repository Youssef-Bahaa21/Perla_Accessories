{
    "version": 2,
    "framework": "angular",
    "buildCommand": "npm run build",
    "outputDirectory": "dist/client/browser",
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/products/(\\d+)",
            "dest": "/api/render"
        },
        {
            "src": "/products",
            "dest": "/api/render"
        },
        {
            "src": "/landing",
            "dest": "/api/render"
        },
        {
            "src": "^/$",
            "dest": "/api/render"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}